{
  "manifest_version": 3,
  "name": "In-browser Code Execution for ChatGPT",
  "version": "1.0",
  "description": "In-browser code execution for ChatGPT",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://chatgpt.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://chatgpt.com/*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"],
      "type": "module"
    }
  ],
  "web_accessible_resources": [{
    "matches": ["*://*/*"],
    "resources": []
  }]
}
